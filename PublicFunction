Public Function av(sample1 As Range, sample2 As Range) As Variant
On Error GoTo errorhandler

If VBA.Right(sample1, 1) = "L" And VBA.Right(sample2, 1) = "L" Then
    av = sample1
ElseIf VBA.Right(sample1.Value, 1) = "L" Or VBA.Right(sample2.Value, 1) = "L" Then
    av = "error"
Else
    av = WorksheetFunction.Text(VBA.Round(WorksheetFunction.Average(sample1.Value, sample2.Value), Application.Max(xxd(sample1), xxd(sample2))), "0." & WorksheetFunction.Rept("0", Application.Max(xxd(sample1), xxd(sample2))))
End If

errorhandler:
Exit Function
End Function
Public Function xxd(t As Range) As Integer
On Error GoTo errorhandler

    If InStr(t.Text, ".") Then
        xxd = Len(t.Text) - InStr(t.Text, ".")
    Else
        xxd = Len(t.Text) - Len(Int(t.Value))
    End If

errorhandler:
Exit Function
End Function
Public Sub copysub(arr() As Range, dd As Range)
Dim n() As Integer, tn As Integer, i As Integer, x As Integer, rr As String, rg As Range, arrt As Variant, art As Range
On Error GoTo errorhandler

tn = 0
x = 1
ReDim n(1 To UBound(arr())) As Integer
For i = 1 To UBound(arr())
    n(i) = arr(i).Rows.Count
    tn = tn + n(i)
Next
ReDim arrt(1 To tn) As Variant
With Application.WorksheetFunction
For i = 1 To UBound(arr())
    For Each rg In arr(i)
        If IsError(rg) Then
            arrt(x) = ""
        ElseIf rg = "/" Then
            arrt(x) = ""
        ElseIf Right(rg, 1) = "L" Then
            arrt(x) = VBA.Round(CDec(dd / 2), xxd(dd))
        Else
            arrt(x) = rg
        End If
    x = x + 1
    Next
Next
rr = "F2" & ":" & "F" & tn
    Set art = Sheets("sheet1").Range(rr)
        art.Value = Application.Transpose(arrt)
End With

errorhandler:
Exit Sub
End Sub
Public Sub savesub(xname As String)
Dim nl As String, t As String
On Error GoTo errorhandler

Sheets("sheet1").Select
Sheets("sheet1").Range("a1").Select
nl = Chr(13) + Chr(10)
ActiveWorkbook.SaveAs Filename:=CreateObject("WScript.Shell").SpecialFolders("Desktop") & "\" & xname & Format(Year(Now), yyyy) & "-" & Format(Month(Now), mm) & "-" & Format(Day(Now), dd) & "(" & Format(Hour(Now), hh) & Format(Minute(Now), nn) & ")" & "." & "xls", FileFormat:=xlNormal, Password:="", WriteResPassword:="", ReadOnlyRecommended:=False, CreateBackup:=False
ActiveSheet.OLEObjects("CommandButton1").Object.Caption = "ÎÄ¼þÃû:" & nl & xname & Format(Year(Now), yyyy) & "-" & Format(Month(Now), mm) & "-" & Format(Day(Now), dd) & "(" & Format(Hour(Now), hh) & Format(Minute(Now), nn) & ")" & "." & "xls"

errorhandler:
Exit Sub
End Sub
Public Sub parallelsub(acell As Range, samplerange As Range, qcrange As Range, qcname As String, qcsample1column As String, asname As String, resultcolumn As String, qcsample2column As String)
Dim sr As Range, qcr As Range
On Error GoTo errorhandler

If Intersect(acell, samplerange) Is Nothing Then
    Exit Sub
Else
    For Each qcr In qcrange
        If qcr = "/" Then
            qcr = acell
            Sheets(qcname).Range(qcsample1column & qcr.Row).Formula = "=" & asname & "!" & resultcolumn & acell.Row
            For i = 1 To UBound(samplerange())
                For Each sr In samplerange(i)
                    If sr = "/" Then
                        sr = acell.Value & "-Æ½"
                    Sheets(qcname).Range(qcsample2column & qcr.Row).Formula = "=" & asname & "!" & resultcolumn & sr.Row
                    Exit Sub
                    End If
                Next
            Next
        End If
    Next
End If

errorhandler:
Exit Sub
End Sub
Public Sub recoverysub(projectname As Range, methodname As Range, detectionrange As Range, curvea As Range, curveb As Range, recoveryamount As Variant, recoveryunit As Variant, acell As Range, samplerange As Range, qcoriginrange As Range, qcname As String, qcorigincolumn As String, asname As String, resultcolumn As String, qcrecoveryamountcolumn As String, qcrecoverycolumn As String)
Dim sr As Range, qcr As Range
On Error GoTo errorhandler

If Intersect(acell, samplerange) Is Nothing Then
    Exit Sub
Else
    If recoveryunit = "¦Ìg" Or recoveryunit = "mg" Or recoveryunit = "g" Then
        For Each qcr In qcoriginrange
            If qcr = "/" Then
                qcr = acell
                If Right(ActiveSheet.Range(resultcolumn & acell.Row), 1) = "L" Then
                    Sheets(qcname).Range(qcorigincolumn & qcr.Row) = 0
                    Sheets(qcname).Range(qcorigincolumn & qcr.Row).NumberFormatLocal = "0." & WorksheetFunction.Rept("0", xxd(detectionrange))
                Else
                    Sheets(qcname).Range(qcorigincolumn & qcr.Row).Formula = "=dpe((" & asname & "!" & Chr(Range(resultcolumn & 1).Column - 1 + 64) & acell.Row & "-" & asname & "!" & curvea.Address & ")/" & asname & "!" & curveb.Address & "," & asname & "!" & projectname.Address & "," & asname & "!" & methodname.Address & ")"
                End If
                For i = 1 To UBound(samplerange())
                    For Each sr In samplerange(i)
                        If sr = "/" Then
                            sr = acell.Value & "-¼Ó±ê"
                            Sheets(qcname).Range(qcrecoveryamountcolumn & qcr.Row) = recoveryamount
                            Sheets(qcname).Range(qcrecoveryamountcolumn & qcr.Row).NumberFormatLocal = "0." & WorksheetFunction.Rept("0", xxd(detectionrange))
                            Sheets(qcname).Range(qcrecoverycolumn & qcr.Row).Formula = "=dpe((" & asname & "!" & Chr(Range(resultcolumn & 1).Column - 1 + 64) & sr.Row & "-" & asname & "!" & curvea.Address & ")/" & asname & "!" & curveb.Address & "," & asname & "!" & projectname.Address & "," & asname & "!" & methodname.Address & ")"
                            Sheets(qcname).Range("G10").Formula = "Ô­ÑùÆ·º¬Á¿" & " " & "(" & recoveryunit & ")"
                            Sheets(qcname).Range("H10").Formula = "¼Ó±êº¬Á¿" & "     " & "(" & recoveryunit & ")"
                            Sheets(qcname).Range("I10").Formula = "¼Ó±êºóº¬Á¿" & " " & "(" & recoveryunit & ")"
                            Exit Sub
                        End If
                    Next
                Next
            End If
        Next
    Else
        For Each qcr In qcoriginrange
            If qcr = "/" Then
                qcr = acell
                If Right(ActiveSheet.Range(resultcolumn & acell.Row), 1) = "L" Then
                    Sheets(qcname).Range(qcorigincolumn & qcr.Row) = 0
                    Sheets(qcname).Range(qcorigincolumn & qcr.Row).NumberFormatLocal = "0." & WorksheetFunction.Rept("0", xxd(detectionrange))
                Else
                    Sheets(qcname).Range(qcorigincolumn & qcr.Row).Formula = "=" & asname & "!" & resultcolumn & acell.Row
                End If
                For i = 1 To UBound(samplerange())
                    For Each sr In samplerange(i)
                        If sr = "/" Then
                            sr = acell.Value & "-¼Ó±ê"
                        Sheets(qcname).Range(qcrecoveryamountcolumn & qcr.Row) = recoveryamount
                        Sheets(qcname).Range(qcrecoveryamountcolumn & qcr.Row).NumberFormatLocal = "0." & WorksheetFunction.Rept("0", xxd(detectionrange))
                        Sheets(qcname).Range(qcrecoverycolumn & qcr.Row).Formula = "=" & asname & "!" & resultcolumn & sr.Row
                        Sheets(qcname).Range("G10").Formula = "Ô­ÑùÆ·Å¨¶È" & " " & "(" & recoveryunit & ")"
                        Sheets(qcname).Range("H10").Formula = "¼Ó±êÅ¨¶È" & "     " & "(" & recoveryunit & ")"
                        Sheets(qcname).Range("I10").Formula = "¼Ó±êºóÅ¨¶È" & " " & "(" & recoveryunit & ")"
                        Exit Sub
                        End If
                    Next
                Next
            End If
        Next
    End If
End If

errorhandler:
Exit Sub
End Sub
Public Sub printsub(sname As String, qcname As String)
Dim obj As OLEObject, n As Integer
On Error GoTo errorhandler

For Each obj In ActiveSheet.OLEObjects
    If TypeName(obj.Object) = "CheckBox" Then
        n = n + 1
    End If
Next
For Each obj In ActiveSheet.OLEObjects
    If TypeName(obj.Object) = "CheckBox" Then
        If obj.Object.Value = True Then
            If VBA.Right(obj.name, 1) <= n - 1 Then
                Sheets(sname & VBA.Right(obj.name, 1)).PrintOut
            Else
                Sheets(qcname).PrintOut
            End If
        End If
    End If
Next

errorhandler:
Exit Sub
End Sub
