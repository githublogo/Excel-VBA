Public Type MethodProperty
    analysis_method_Name As String
    analysis_method_Detection As Double
    analysis_method_Significantfigure As Integer
    analysis_method_Decimal As Integer
End Type

Public Type MethodLibrary
    analysis_project_Name As String
    analysis_method_Version(1 To 3) As MethodProperty
End Type

Public Type WorksheetProperty
    sheet_excel_Name As String
    sheet_sheet_Name As String
    sheet_Index As Integer
    sheet_blank_Sample As Integer
    sheet_sample_Column As String
    sheet_sample_Rowbegin As Integer
    sheet_sample_Rowend As Integer
    sheet_sample_Range() As Range
    sheet_result_Column As String
    sheet_result_Rowbegin As Integer
    sheet_result_Rowend As Integer
    sheet_result_Range() As Range
    sheet_curve_Y As Range
    sheet_curve_X As Range
    sheet_analysis_project_Cell As Range
    sheet_analysis_method_Cell As Range
    sheet_analysis_Detection_Cell  As Range
    sheet_quality_control_Name As String
    sheet_quality_control_parallel_Range As Range
    sheet_quality_control_parallel_Column As String
    sheet_quality_control_parallel_sample1_Column As String
    sheet_quality_control_parallel_sample2_Column As String
    sheet_quality_control_parallel_Rowbegin As Integer
    sheet_quality_control_parallel_Rowend As Integer
    sheet_quality_control_standard_Range As Range
    sheet_quality_control_standard_Column As String
    sheet_quality_control_standard_Rowbegin As Integer
    sheet_quality_control_standard_Rowend As Integer
End Type


